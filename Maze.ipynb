   "source": [
    "class QueueFrontier():\n",
    "    def remove(self):\n",
    "        if self.empty():\n",
    "            raise Exception(\"Empty Frontier\")\n",
    "        else:\n",
    "            node = self.frontier[0]\n",
    "            self.frontier = self.frontier[1:]\n",
    "            return node"
   ]
